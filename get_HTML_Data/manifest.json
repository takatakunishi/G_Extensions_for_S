{
    "manifest_version": 2,
    "name": "getXML",
    "version": "0.1",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "content/content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "event/event.js"
        ],
        "persistent": true
    }
}